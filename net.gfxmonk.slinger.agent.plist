<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<!-- launchd agent definition for slinger -->
<plist version="1.0">
  <dict>

    <key>Label</key>
    <string>net.gfxmonk.slinger.agent</string>

    <key>RunAtLoad</key>
    <true/>

    <key>Keepalive</key>
    <true/>

    <key>ProgramArguments</key>
    <array>
      <string>/Users/tcuthbertson/Code/timbertson/slinger/.build/release/Slinger</string>
    </array>

    <key>StandardOutPath</key>
    <string>/Users/tcuthbertson/.tmp/slinger.log</string>
    <key>StandardErrorPath</key>
    <string>/Users/tcuthbertson/.tmp/slinger.log</string>


  </dict>
</plist>
